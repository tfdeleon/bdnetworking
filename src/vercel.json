{
  "version": 2,
  "builds": [
    {
      "src": "src/server/api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/available-times",
      "dest": "src/server/api/available-times.js"
    },
    {
      "src": "/api/book",
      "dest": "src/server/api/book.js"
    },
    {
      "src": "/api/verify-recaptcha",
      "dest": "src/server/api/verify-recaptcha.js"
    }
  ]
}
